{
  "name": "pretty-quick",
  "version": "4.1.1",
  "description": "Get Pretty Quick",
  "repository": "prettier/pretty-quick",
  "author": "Lucas Azzola <@azz>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "bin": "lib/cli.mjs",
  "main": "lib/index.js",
  "module": "lib/index.esm.mjs",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "default": "./lib/index.esm.mjs"
    },
    "./*": "./lib/*.js",
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "files": [
    "bin",
    "img",
    "lib",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "git",
    "mercurial",
    "hg",
    "prettier",
    "pretty-quick",
    "formatting",
    "code",
    "vcs",
    "precommit"
  ],
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "dependencies": {
    "find-up": "^5.0.0",
    "ignore": "^7.0.3",
    "mri": "^1.2.0",
    "picocolors": "^1.1.1",
    "picomatch": "^4.0.2",
    "tinyexec": "^0.3.2",
    "tslib": "^2.8.1"
  }
}